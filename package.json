{
    "name": "@supervaize/n8n-nodes-supervaizer",
    "version": "0.1.3",
    "description": "n8n nodes for Supervaize integration",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://supervaize.com",
    "author": {
        "name": "Supervaize",
        "email": "contact@supervaize.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supervaize/n8n-nodes-supervaizer.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier --write \"**/*.{ts,js,md}\"",
        "lint": "eslint \"**/*.{ts,js}\"",
        "lint:fix": "eslint \"**/*.{ts,js}\" --fix",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/SupervaizeApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Supervaize/SupervaizeAgentRegistration.node.js",
            "dist/nodes/SupervaizeTrigger/SupervaizeTrigger.node.js",
            "dist/nodes/SupervaizeTrigger/SupervaizeCommandTrigger.node.js",
            "dist/nodes/Supervaize/SupervaizeEvent.node.js",
            "dist/nodes/Supervaize/SupervaizeHumanInLoop.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/jest": "^27.0.2",
        "@types/node": "^14.17.0",
        "@types/request-promise-native": "^1.0.17",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.0",
        "gulp": "^4.0.2",
        "jest": "^27.3.1",
        "n8n-core": "^0.130.0",
        "n8n-workflow": "^0.117.0",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "axios": "^0.24.0"
    }
}
